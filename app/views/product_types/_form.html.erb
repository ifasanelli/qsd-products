<%= form_with model: @product_type, local: true do |f| %>
 <%= f.label :name, 'Nome' %>
 <%= f.text_field :name %>
 <%= f.label :description, 'Descrição' %>
 <%= f.text_area :description %>
 <%= f.label :product_key, 'Chave do Produto' %>
 <%= f.text_field :product_key %>
 <%= f.submit 'Enviar' %>
<% end %>